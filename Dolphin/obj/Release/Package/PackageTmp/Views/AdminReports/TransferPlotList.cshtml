@model Dolphin.Models.Reports

@{
    ViewBag.Title = "TransferPlotList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


@using (Html.BeginForm("TransferPlotList", "AdminReports", FormMethod.Post, new { @id = "form1" }))
{


    <div class="page-header">
        <div class="page-block">
            <div class="row align-items-center">
                <div class="col-md-8">
                    <div class="page-header-title">
                        <h4 class="m-b-10">Transfer Plot List</h4>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <div class="row">
                        <div class="col-md-12">

                            <!-- HTML5 Export Buttons table start -->
                            <div class="card">
                                <div class="card-header table-card-header">
                                    <p style="color:red">@TempData["TransferPlotList"]</p>
                                </div>
                                <div class="card-block">
                                    <div class="dt-responsive table-responsive">
                                        <table id="basic-btn" class="table table-striped table-bordered nowrap">
                                            <thead>
                                                <tr>
                                                    <th>Sr.No.</th>
                                                    <th>Old Transfer Plot Details</th>
                                                    <th>New Transfer Plot Details</th>
                                                    <th>Transfer By</th>
                                                    <th>Transfer Date</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @*@if (Model != null)
                                                {
                                                    if (Model.lstPlotAvailabilityReport != null)
                                                    {
                                                        int i = 1;
                                                        foreach (var Item in Model.lstPlotAvailabilityReport)
                                                        {
                                                            <tr class="text-left">
                                                                <td>@i</td>
                                                                <td>@Item.SiteName</td>
                                                                <td>@Item.PlotNumber</td>
                                                                <td>@Item.PlotArea</td>
                                                            </tr>
                                                            i++;
                                                        }
                                                    }
                                                }*@

                                            </tbody>
                                           
                                        </table>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


}



