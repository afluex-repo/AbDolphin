@model Dolphin.Models.Reports

@{
    ViewBag.Title = "TDSReportDateWise";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


@using (Html.BeginForm("TDSReportDateWise", "AdminReports", FormMethod.Post, new { @id = "form1", name = "formKYC" }))
{

    <div class="page-content">
        <div class="card">
            <div class="card-body">
                <div class="d-flex align-items-center justify-content-between">
                    <h5 class="mb-0">TDS Report DateWise</h5>
                </div>
                @*<p style="color: red;display: block; margin-bottom:0px; text-align:center">
                    @TempData["KYCVerification"]
                </p>*@
            </div>
        </div>
        <div class="items">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                      
                        <div class="col-md-2 mb-3">
                            <div class="form-group">
                                <label> From Date</label>
                                @Html.TextBoxFor(m => m.FromDate, new { @class = "form-control", @readonly = "ds", @placeholder = "DD/MM/YYYY" })
                            </div>
                        </div>
                        <div class="col-md-2 mb-3">
                            <div class="form-group">
                                <label>  To Date</label>
                                @Html.TextBoxFor(m => m.ToDate, new { @class = "form-control", @readonly = "ds", @placeholder = "DD/MM/YYYY" })
                            </div>
                        </div>
                        

                        <div class="col-md-2 mb-3">
                            <div class="form-group">
                                <label class="d-block">  &nbsp;</label>
                                <input class="btn btn-success" type="submit" value="Search" id="btnSearch" name="btnSearch" />

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table id="example2" class="table table-striped table-bordered">
                        <thead>
                            <tr class="table-active">
                                <th>Sr No.</th>
                                <th>LoginID</th>
                                <th>Name</th>
                                <th>Pan Number</th>
                                <th>TDS</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                if (Model.lstTDSReportDateWise != null)
                                {
                                    int i = 1;
                                    foreach (var Item in Model.lstTDSReportDateWise)
                                    {
                                        <tr class="text-left">
                                            <td>@i</td>
                                            <td>@Item.LoginId</td>
                                            <td>@Item.Name</td>
                                            <td>@Item.PanNumber</td>
                                            <td>@Item.TDS</td>
                                        </tr>

                                        i++;
                                    }
                                }
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
}
<script src="~/files/bower_components/jquery/js/jquery.min.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>

    $('#FromDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,
    });
    $('#ToDate').datepicker({
        dateFormat: 'dd/mm/yy',
        changeYear: true,
        changeMonth: true,
    });
</script>

