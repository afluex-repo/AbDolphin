@{
    ViewBag.Title = "CompanyOverview";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Sub banner start -->
<div class="content-wrapper">

    <div class="breadcrumb-wrap bg-f companyOverview">
        <div class="container">
            <div class="breadcrumb-title">
                <h2>Company Overview</h2>
                <ul class="breadcrumb-menu list-style">
                    <li><a href="/Website/Index">Home </a></li>
                    <li>Company Overview</li>
                </ul>
            </div>
        </div>
    </div>


    <section class="about-wrap style2  pt-100 pb-100 bg-seashell">
        <div class="container">
            <div class="about-box">
                <img src="~/assetsWeb/img/about/about-shape-3.png" alt="Image" class="about-shape-three">
                <div class="row gx-5 align-items-center">
                    <div class="col-lg-5" data-aos="fade-right" data-aos-duration="1200" data-aos-delay="200">
                        <div class="">
                            <img src="~/assetsWeb/img/gallery/g4.jpg" class="rounded" alt="">
                        </div>
                    </div>
                    <div class="col-lg-7" data-aos="fade-left" data-aos-duration="1200" data-aos-delay="200">
                        <div class="about-content">
                            <div class="content-title style2">
                                <span>About</span>
                                <h2>Abdolphin Infratech</h2>
                                <p>
                                    At Abdolphin Infratech, we believe in turning dreams into addresses. We are more than just a real estate company; we are the architects of your aspirations. Our journey began with a vision to redefine the real estate landscape, and today, we stand as a beacon of trust and excellence in the industry.
                                </p>
                                <p>
                                    Our vision is to be the preferred choice for all your real estate needs, offering innovative and customized solutions that exceed your expectations. We believe in creating value for our clients by harnessing our in-depth market knowledge, cutting-edge technology, and a passion for excellence.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>



