@model Dolphin.Models.Master
@{
    ViewBag.Title = "RankList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

@using (Html.BeginForm("RankList", "Master", FormMethod.Post, new { @id = "form1" }))
{ 
    <p style="color: red">
        @TempData["Rank"]
    </p>

    <div class="page-content">
        <div class="card">
            <div class="card-body">
                <div class="d-flex align-items-center justify-content-between">
                    <h5 class="mb-0">Rank List</h5>
                </div>
                @*<p style="color: red;display: block; margin-bottom:0px; text-align:center">
                    @TempData["BlockUnblock"]
                </p>*@
            </div>
        </div>
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table id="example2" class="table table-striped table-bordered">
                        <thead>
                            <tr class="table-active">
                                <th>Sr No.</th>
                                <th>Rank Name</th>
                                <th>Percentage</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                if (Model.lstBlock1 != null)
                                {
                                    int i = 1;
                                    foreach (var Item in Model.lstBlock1)
                                    {
                                        <tr class="text-left">
                                            <td>@i</td>
                                            <td>@Item.DesignationName</td>
                                            <td>@Item.Percentage</td>
                                            <td>
                                                @*   <a href="/Master/RankMaster?DesignationID=@Item.EncryptKey" class="btn btn-info btn-sm">Edit</a>*@
                                                <a href="/Master/DeleteRank?id=@Item.DesignationID" class="btn btn-danger btn-sm" onclick="return confirm('Are you sure you wish to delete this Rank?');">Delete</a>
                                            </td>

                                        </tr>

                                        i++;
                                    }
                                }
                            }


                        </tbody>
                        <tfoot>
                            <tr class="table-active">
                                <th>Sr No.</th>
                                <th>Rank Name</th>
                                <th>Percentage</th>
                                <th></th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>

}
<script src="~/files/bower_components/jquery/js/jquery.min.js"></script>
<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">



